*,
*:before,
*:after {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
html,
body {
  height: 100%;
  font-family: 'Open Sans', sans-serif;
  font-size: 93.75%;
  line-height: 1.333;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  -ms-content-zooming: none;
}
.wrapper {
  min-height: 100%;
  margin-bottom: -5em;
  padding-bottom: 5em;
}
figure {
  margin: 0;
}
a {
  text-decoration: none;
  color: #008fcc;
}
a:hover,
a:focus {
  text-decoration: underline;
}
a img {
  vertical-align: bottom;
}
hr {
  font-size: 15px;
  /* @alternate */ font-size: 1rem;
  margin: 0;
  height: 1px;
  border: none;
  border-top: 1px solid #ccc;
}
h2 {
  font-size: 2.4em;
  font-weight: bold;
  color: #008fcc;
  margin-top: 0.7em;
  margin-bottom: 0.7em;
}
h3 {
  font-size: 1.6em;
  font-weight: bold;
  color: #008fcc;
  margin: 0 0 0.5em;
}
h4 {
  font-size: 1.333333333333333em;
  font-weight: bold;
  color: #008fcc;
}
h5 {
  font-size: 1.066666666666667em;
  font-weight: 600;
  color: #008fcc;
}
figure img {
  display: block;
}
figcaption {
  margin-top: 1.333333333333333em;
}
.grid,
.stackenblochen {
  display: block;
  width: 100%;
  max-width: 80rem;
  list-style: none;
  margin: 0 auto;
  zoom: 1;
}
.grid:before,
.stackenblochen:before,
.grid:after,
.stackenblochen:after {
  content: "";
  display: table;
}
.grid:after,
.stackenblochen:after {
  clear: both;
}
.grid *,
.stackenblochen * {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.grid .grid,
.stackenblochen .grid,
.grid .stackenblochen,
.stackenblochen .stackenblochen {
  width: auto;
  max-width: none;
  margin-left: -1rem;
  margin-right: -1rem;
}

.navbar {
  font-size: 15px;
  /* @alternate */ font-size: 1rem;
  padding: 1.333333333333333em 0;
  border-bottom: 1px solid #eee;
}
.navbar_button {
  display: inline-block;
  font-weight: 600;
  color: #666;
  border: none;
  padding: 0.666666666666667em 1.133333333333333em;
  background-color: #eee;
  -webkit-transition: background-color 0.2s;
  -moz-transition: background-color 0.2s;
  -o-transition: background-color 0.2s;
  -ms-transition: background-color 0.2s;
  transition: background-color 0.2s;
  text-decoration: none;
  text-transform: uppercase;
  margin-right: 1em;
}
.navbar_button:hover,
.navbar_button:focus {
  background-color: #d6d6d6;
  outline: none;
  text-decoration: none;
}
.navbar_buttonactive,
.navbar_buttonactive:focus {
  color: #fff;
  background-color: #f0ab00;
}
.navbar_buttonactive:hover {
  background-color: #d89a00;
}
.button {
  font-size: 15px;
  /* @alternate */ font-size: 1rem;
  display: inline-block;
  font-weight: 600;
  color: #fff;
  border: none;
  padding: 0.666666666666667em 1.133333333333333em;
  background-color: #f0ab00;
  -webkit-transition: background-color 0.2s;
  -moz-transition: background-color 0.2s;
  -o-transition: background-color 0.2s;
  -ms-transition: background-color 0.2s;
  transition: background-color 0.2s;
  text-decoration: none;
}
.button:hover,
.button:focus {
  background-color: #d89a00;
  outline: none;
  text-decoration: none;
}
.buttoninactive {
  background-color: #ccc;
}
.buttoninactive:hover {
  background-color: #b8b8b8;
}
.buttonrefresh {
  background-image: url("data:image/png;base64,");
}
.buttonrefresh,
.buttonsearch,
.buttonarrowdown {
  background-repeat: no-repeat;
  background-position: left top;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  background-size: cover;
  width: 2.333333333333333em;
  height: 0px;
  padding: 2.333333333333333em 0 0 0;
  border: 0;
  overflow: hidden;
}
.buttonsearch {
  background-image: url("data:image/png;base64,");
  background-color: transparent;
  width: 1.333333333333333em;
  padding: 1.4em 0 0 0;
}
.buttonarrowdown {
  background-image: url("data:image/png;base64,");
  width: 2.666666666666667em;
  padding: 2.733333333333333em 0 0 0;
}
.label {
  display: inline-block;
  font-size: 11.25px;
  /* @alternate */ font-size: 0.75rem;
  font-weight: 600;
  color: #fff;
  padding: 0.133333333333333em 0.933333333333333em;
}
.label[data-labeltype='live'] {
  background-color: #f00;
  vertical-align: middle;
  margin-top: -0.666666666666667em;
}
.label[data-labeltype='finished'] {
  background-color: #8ab54e;
  vertical-align: middle;
  margin-top: -0.666666666666667em;
}
.label[data-labeltype='new'],
.label[data-labeltype='updated'] {
  background-color: #f0ab00;
  text-transform: uppercase;
  vertical-align: middle;
}
.label[data-labeltype='group1'] {
  background-color: #a71724;
}
.label[data-labeltype='group2'] {
  background-color: #c79242;
}
.label[data-labeltype='group3'] {
  background-color: #005fa7;
}
.morelink {
  color: #008fcc;
  text-transform: uppercase;
  text-align: center;
  font-size: 0.933333333333333em;
  font-weight: 700;
  padding: 1.866666666666667em;
  display: block;
  text-decoration: none;
}
.morelink:hover,
.morelink:focus {
  text-decoration: underline;
}
.dummy {
  font-size: 15px;
  /* @alternate */ font-size: 1rem;
  min-height: 25em;
  position: relative;
}
.dummy:after {
  content: 'Dummy content';
  position: absolute;
  top: 50%;
  left: 50%;
  font-weight: 700;
  font-size: 6em;
  color: #f7f7f7;
  text-shadow: 1px 1px 0 #eee, 2px 2px 0 #eee, 3px 3px 0 #eee, 4px 4px 0 #eee, 5px 5px 0 #eee, 6px 6px 0 #eee, 7px 7px 0 #eee;
  margin: -100px 0 0 -350px;
}
.mainsection {
  padding-top: 5.333333333333333em;
  padding-bottom: 5.333333333333333em;
}
.mainsection_header {
  text-align: center;
  margin-bottom: 3.666666666666667em;
}
.mainsection_header p {
  margin-top: 0;
  font-size: 1.2em;
}
.mainsection_header_title {
  margin: 0;
  font-size: 2.4em;
  text-align: center;
}
.mainsection_navigation {
  text-align: center;
}
.lightbox {
  font-size: 15px;
  /* @alternate */ font-size: 1rem;
}
.lightbox_overlay {
  font-size: 15px;
  /* @alternate */ font-size: 1rem;
  background: #000;
  opacity: 0.7;
  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=70)";
  filter: alpha(opacity=70);
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 2000;
  -webkit-transition: opacity 0.3s ease;
  -moz-transition: opacity 0.3s ease;
  -o-transition: opacity 0.3s ease;
  -ms-transition: opacity 0.3s ease;
  transition: opacity 0.3s ease;
}
.lightbox_overlayhidden {
  opacity: 0;
  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
  filter: alpha(opacity=0);
}
.lightbox_content {
  position: fixed;
  width: 100%;
  height: 100%;
  opacity: 1;
  -ms-filter: none;
  filter: none;
  top: 0;
  left: 0;
  z-index: 2001;
  background-repeat: no-repeat;
  background-position: center center;
  -webkit-transition: opacity 0.3s ease;
  -moz-transition: opacity 0.3s ease;
  -o-transition: opacity 0.3s ease;
  -ms-transition: opacity 0.3s ease;
  transition: opacity 0.3s ease;
}
.lightbox_contenthidden {
  opacity: 0;
  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
  filter: alpha(opacity=0);
}
.lightbox_content_enlargeditem {
  -webkit-transition: margin 0.7s linear;
  -moz-transition: margin 0.7s linear;
  -o-transition: margin 0.7s linear;
  -ms-transition: margin 0.7s linear;
  transition: margin 0.7s linear;
  display: inline-block;
  position: absolute;
  left: 50%;
  top: 50%;
}
.lightbox_content_enlargeditem_caption {
  text-align: center;
  color: #f6f9fc;
  font-weight: 700;
}
.lightbox_content_enlargeditem_close {
  display: inline-block;
  background-image: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAQCAYAAAAf8/9hAAAAGXRFWHRTb2Z0d2FyZQBBZG9iZSBJbWFnZVJlYWR5ccllPAAAAHVJREFUeNqsk4EJACEIReWawY2bMLh5LME7JNQEE4So/x9pCUTUVvaVuBKSieJpIAuOkYSgaDn6t/EmIdrMHtQHJ4g2/5pd4EFM8w7whK7ZAliQEZWWaVbY3AeqcbuEUhMj4fEZ8XTV6CNlzC7kyjCVxnkKMABEXRUAnXsRRQAAAABJRU5ErkJggg==");
  background-repeat: no-repeat;
  background-position: center center;
  width: 1.333333333333333em;
  height: 0;
  overflow: hidden;
  padding-top: 1.333333333333333em;
  position: absolute;
  top: 1.333333333333333em;
  right: 1.333333333333333em;
  cursor: pointer;
  z-index: 2003;
}
.lightbox_content .media_slideshow_controls {
  z-index: 2002;
}
.lightbox_data {
  display: none;
}
.lightbox_item {
  font-size: 15px;
  /* @alternate */ font-size: 1rem;
}
