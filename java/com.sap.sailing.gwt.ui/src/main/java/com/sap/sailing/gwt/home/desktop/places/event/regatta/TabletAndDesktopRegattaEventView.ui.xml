<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
		xmlns:c="urn:import:com.sap.sailing.gwt.common.client.controls.tabbar"
		xmlns:dp="urn:import:com.sap.sailing.gwt.home.desktop.partials"
		xmlns:e="urn:import:com.sap.sailing.gwt.home.desktop.places.event.regatta">
	<ui:with field="i18n" type="com.sap.sailing.gwt.ui.client.StringMessages" />
	<g:HTMLPanel>
		<c:TabPanel ui:field="tabPanelUi">
			<c:header>
				<dp:eventheader.EventHeader ui:field="eventHeader" />
			</c:header>
 			<c:tabcontent title="{i18n.overview}">
 				<e:overviewtab.RegattaOverviewTabView ui:field="regattaOverviewTabUi" />
 			</c:tabcontent>
			<c:tabcontent title="{i18n.racesAndTracking}">
				<e:racestab.RegattaRacesTabView ui:field="racesTabUi" />
			</c:tabcontent>
			<c:tabcontent title="{i18n.leaderboard}">
				<e:leaderboardtab.RegattaLeaderboardTabView />
			</c:tabcontent>
			<c:tabcontent title="{i18n.competitorsAnalytics}">
				<e:analyticstab.RegattaCompetitorAnalyticsTabView />
			</c:tabcontent>
			<c:tabcontent title="{i18n.media}">
				<e:mediatab.RegattaMediaTabView />
			</c:tabcontent>
		</c:TabPanel>
	</g:HTMLPanel>
</ui:UiBinder> 