<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared">
  <ui:with field="i18n" type="com.sap.sailing.gwt.ui.client.StringMessages" />
  <ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
  <ui:with field="local_res" type="com.sap.sailing.gwt.home.desktop.partials.userHeader.UserHeaderResources" />
  <g:HTMLPanel>
    <div class="{local_res.css.userheader}">
      <div class="{res.mediaCss.grid}">
        <div class="{res.mediaCss.small12} {res.mediaCss.large9} {res.mediaCss.columns}">
          <div class="{local_res.css.userheader_intro}">
            <div class="{local_res.css.userheader_intro_logo}">
              <div ui:field="imageUi" class="{local_res.css.userheader_intro_logo_image}"></div>
            </div>
            <div class="{local_res.css.userheader_intro_title}">
              <div ui:field="nameUi" class="{local_res.css.userheader_intro_title_realname}" />
              <div ui:field="usernameUi" class="{local_res.css.userheader_intro_title_username}" />
            </div>
          </div>
        </div>
      </div>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>