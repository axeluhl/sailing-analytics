<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="i18n" type="com.sap.sailing.gwt.ui.client.StringMessages" />
	<ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
	<ui:with field="local_res" type="com.sap.sailing.gwt.home.desktop.partials.regattanavigation.RegattaNavigationResources" />
	<g:FlowPanel addStyleNames="{res.mediaCss.grid} {local_res.css.regattanavigation}" >
        <g:FlowPanel addStyleNames="{res.mediaCss.small12} {res.mediaCss.medium7} {res.mediaCss.columns}" ui:field="actionContainerUi">
        	<g:Label addStyleNames="{local_res.css.regattanavigation_text}">
        		<ui:text from='{i18n.display}' />:
        	</g:Label>
        </g:FlowPanel>
        <g:SimplePanel addStyleNames="{res.mediaCss.small12} {res.mediaCss.medium5} {res.mediaCss.columns}" ui:field="additionalWidgetContainerUi"/>
	</g:FlowPanel>
</ui:UiBinder> 