<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared">
	<ui:with field="i18n" type="com.sap.sailing.gwt.common.client.i18n.TextMessages" />
	<ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
    <ui:with field="local_res" type="com.sap.sailing.gwt.home.client.shared.event.EventTeaserResources" />
  <g:HTMLPanel>
    <div class="{local_res.css.eventteasercontainer}">
      <a ui:field="eventLink" class="{local_res.css.eventteaser}">
        <div ui:field="eventImage" class="{local_res.css.eventteaser_image}"></div>
        <div class="{local_res.css.eventteaser_title}">
          <div class="{local_res.css.eventteaser_title_name}">
            <div ui:field="eventState" data-labeltype="live" class="{res.mainCss.label} {res.mainCss.labellarge} {local_res.css.eventteaser_title_name_label}" style="margin-top: 0px;" />
            <span ui:field="eventName" />
          </div>
        </div>
        <div class="{local_res.css.eventteaser_location}">
          <span ui:field="venue" />,&nbsp;<span ui:field="eventDate" />
        </div>
      </a>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>