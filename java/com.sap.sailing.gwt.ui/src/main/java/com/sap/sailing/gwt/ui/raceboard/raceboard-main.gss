.usermanagement_icon {
	width: 3rem;
    height: 3rem;
    vertical-align: middle;
    background-repeat: no-repeat;
    background-position: center center;
    background-size: contain;
    background-image: url('images/home/loggedout.svg');
    cursor: pointer;
    position: absolute;
    right: 10px;
    top: 6px;
}
.usermanagement_icon.usermanagement_open {
	background-image: url('images/home/loggedout-open.svg');
}
.usermanagement_icon.usermanagement_loggedin {
	background-image: url('images/home/loggedin.svg');
}
.usermanagement_icon.usermanagement_loggedin.usermanagement_open {
	background-image: url('images/home/loggedin-open.svg');
}
.usermanagement_view {
	top: 60px !important;
    left: inherit !important;
    right: -0.25em !important;
    z-index: 10;
    line-height: 1.333;
}
.usermanagement_view * {
    font-size: 15px;
}
.usermanagement_view.usermanagement_mobile {
	top: 60px !important;
    left: 0 !important;
}
.usermanagement_mobile .usermanagement_view_content_wrapper {
	padding: 0;
}
.usermanagement_mobile .usermanagement_view_content {
	width: 100%;
}
