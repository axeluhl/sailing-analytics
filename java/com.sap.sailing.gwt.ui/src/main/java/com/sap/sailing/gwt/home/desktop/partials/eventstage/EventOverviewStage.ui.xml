<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" 
		xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared" 
		xmlns:sp="urn:import:com.sap.sailing.gwt.home.shared.partials" 
		xmlns:dp="urn:import:com.sap.sailing.gwt.home.desktop.partials">
	<ui:with field="i18n" type="com.sap.sailing.gwt.ui.client.StringMessages" />
	<ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
	<g:HTMLPanel>
		<div class="{res.mainCss.spacer} {res.mainCss.spacersmall}"></div>
		<sp:message.Message ui:field="message" />
		<div class="{res.mediaCss.grid}">
			<g:SimplePanel ui:field="stage" addStyleNames="{res.mediaCss.small12} {res.mediaCss.columns}" />
			<div ui:field="updatesWrapperUi" class="{res.mediaCss.small12} {res.mediaCss.medium5} {res.mediaCss.large4} {res.mediaCss.columns}" style="display: none;">
				<dp:updates.UpdatesBox ui:field="updatesUi" addStyleNames="{res.mainCss.spacermargin} {res.mainCss.spacermargintopsmall} {res.mainCss.spacermarginonlyonsmall}" />
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>