<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:popups="urn:import:com.sap.sailing.dashboards.gwt.client.popups.competitorselection"
	xmlns:mgwts="urn:import:com.googlecode.mgwt.ui.client.widget.panel.scroll">

	<ui:style
		type='com.sap.sailing.dashboards.gwt.client.popups.competitorselection.CompetitorSelectionPopup.CompetitorSelectionPopupStyle'
		src='CompetitorSelectionPopup.css'></ui:style>

	<g:VerticalPanel ui:field="competitorselectionpopup"
		styleName="{style.competitorselectionpopup}">
		<g:HTMLPanel ui:field="header" styleName="{style.header}">Select your Team</g:HTMLPanel>
		<mgwts:ScrollPanel styleName="{style.scrollpanel}"
			scrollingEnabledX="false">
			<g:FlowPanel>
				<popups:CompetitorTable ui:field="competitortable"></popups:CompetitorTable>
			</g:FlowPanel>
		</mgwts:ScrollPanel>
		<g:Button ui:field="button" styleName="{style.button}"></g:Button>
	</g:VerticalPanel>

</ui:UiBinder> 